          <div class="col-md-3"></div>
          <div class="col-md-6"> 
        
                <form name="form1" method="POST" action="/signup/add" class="signup" onsubmit="return!! (ValidateEmail(document.form1.email))||(CheckPassword(document.form1.password))||(matchPassword())">
                  <h2>Create an account</h2>
                  <h3>Already have an account!!<a href="/login"><span>Sign in</span></a></h3>
                  <div class="signup__field">
                    <input class="signup__input" type="text" name="username" id="username" required />
                    <label class="signup__label" for="username">Username</label>
                  </div>
                
                  <div class="signup__field">
                    <input class="signup__input" type="text" name="email"  id="email" required />
                    <label class="signup__label" for="email">Email</label>
                  </div>
                
                  <div class="signup__field">
                    <input class="signup__input" type="password" name="password"  title="The password must contain atleast one uppercase,one lower case,one digit and one special character" id="passwordInput" required />
                    <label class="signup__label" for="password">Password</label>
                  </div>

                  <div class="signup__field">
                    <input class="signup__input" type="password" name="confirmpassword" id="confirmpasswordInput" required />
                    <label class="signup__label" for="confirmpassword">Confirm Password</label>
                  </div>
                
                  <button>Sign up</button>
                </form>
          </div>
          <!--/signup-->
        

